public class DebugTwo2
{
    //the curlybrackets need to be moved up a level
    
public static void main(String[] args) //squarebrackets after String not args
    {
      int a, b;
      a = 7; // semicolon missing -- maybe I introduced this by accident
      b = 4;

      //math operations should be in their own parenthesis
      System.out.println("The sum is " + (a + b));
      System.out.println("The difference is " + (a - b));
      System.out.println("The product is " + (a * b)); //missing semicolon
    }
}
